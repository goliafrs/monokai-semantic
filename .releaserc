{
  "branches": "master",
  "ci": true,
  "debug": false,
  "dryRun": false,
  "tagFormat": "v${version}",
  "repositoryUrl": "https://github.com/goliafrs/monokai.git",
  "plugins": [
    [
      "@semantic-release/commit-analyzer"
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/git",
      {
        "message": "Release: v${nextRelease.version}",
        "assets": [
          "package.json",
          "package-lock.json"
        ]
      }
    ],
    [
      "@semantic-release/github"
    ]
  ]
}
